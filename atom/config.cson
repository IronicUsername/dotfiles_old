"*":
  welcome:
    showOnStartup: false
  core:
    themes: [
      "one-dark-ui"
      "oceanic-next"
    ]
    disabledPackages: [
      "metrics"
      "exception-reporting"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "*.pyc"
    ]
  editor:
    invisibles: {}
    fontSize: 21
    fontFamily: "SourceCodePro-Light"
    scrollPastEnd: true
    showIndentGuide: true
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  linter:
    showErrorPanel: false
  "linter-eslint":
    useGlobalEslint: true
    globalNodePath: "/Users/jpadilla/.nvm/versions/node/v4.2.1"
    eslintrcPath: "/Users/jpadilla/.eslintrc"
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
  "linter-flake8":
    executablePath: "/Users/jpadilla/.pyenv/versions/2.7.9/bin/flake8"
  "language-babel": {}
".html.php.text":
  editor:
    tabLength: 4
